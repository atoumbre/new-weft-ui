{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "types": ["svelte"],
    "allowJs": true,
    "checkJs": false
  },
  "include": [
    "src/**/*.svelte",
    "src/**/*.ts",
    "vite.config.ts"
  ],
  "exclude": [
    "node_modules",
    "internal_modules",
    ".svelte-kit",
    "dist",
    "build"
  ]
}
